package main

import (
	"fmt"
	"strings"
)

type mapC map[string]string

func SplitCookie(cookie string) (mapC) {
	m := make(map[string]string
	splitCookie := strings.Split(cookie, "; ")
	for i := range splitCookie {
		keyelem := strings.Split(splitCookie[i], "=")
		m[keyelem[0]] = keyelem[1]
	}
	return m

}

